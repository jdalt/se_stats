      ]
    },
    "bsbops": {
      "abbrev": "OPS",
      "formatter": "float_3",
      "formula": "(function () { var slg = 0.0; var obp = 0.0; if (acc.values['bsbab'] + acc.values['bsbbb'] + acc.values['bsbhbp'] + acc.values['bsbsac'] > 0) { obp = ((acc.values['bsbhit'] + acc.values['bsbbb'] + acc.values['bsbhbp']) / (acc.values['bsbab'] + acc.values['bsbbb'] + acc.values['bsbhbp'] + acc.values['bsbsac']) );} if (acc.values['bsbab'] > 0) { slg = (acc.values['bsb1b'] + (acc.values['bsb2b'] * 2) + (acc.values['bsb3b'] * 3) + (acc.values['bsbhr'] * 4)) / acc.values['bsbab'];} return obp + slg; })()",
