      "formatter": "decimal_3",
      "formula": "(function () { if (acc.values['bsbab'] + acc.values['bsbhbp'] + acc.values['bsbbb'] + acc.values['bsbsac'] == 0){ return 0 } else{ return ((acc.values['bsbhit'] + acc.values['bsbbb'] + acc.values['bsbhbp']) /   (acc.values['bsbab'] + acc.values['bsbhbp'] + acc.values['bsbbb'] + acc.values['bsbsac'])   )}})()",
