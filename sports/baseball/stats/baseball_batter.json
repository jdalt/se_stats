    "bsbslg": {
      "abbrev": "SLG",
      "formatter": "float_or_decimal_3",
      "formula": "(function () { if (acc.values['bsbab'] == 0){ return 0 } else{ return ((acc.values['bsb1b'] + (acc.values['bsb2b'] * 2) + (acc.values['bsb3b'] * 3) + (acc.values['bsbhr'] * 4)) / acc.values['bsbab'])}})()",
